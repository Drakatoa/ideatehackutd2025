# Copy this file to .env.local and fill in your actual values
# DO NOT commit .env.local to Git!

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# NVIDIA Nemotron API
NEMOTRON_API_KEY=your_nvidia_api_key
NEMOTRON_API_URL=https://integrate.api.nvidia.com
NEMOTRON_TEXT_MODEL_ID=nvidia/nvidia-nemotron-nano-9b-v2
NEMOTRON_VISION_MODEL_ID=meta/llama-3.2-11b-vision-instruct

# Optional: Retry and timeout settings
# NEMOTRON_MAX_RETRIES=3
# NEMOTRON_TIMEOUT=30000

